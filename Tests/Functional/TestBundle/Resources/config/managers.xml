<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="spomky_test.ip_manager" class="Spomky\IpFilterBundle\Tests\Functional\TestBundle\Manager\IpManager">
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument>%spomky_ip_filter.ip.class%</argument>
        </service>
        <service id="spomky_test.range_manager" class="Spomky\IpFilterBundle\Tests\Functional\TestBundle\Manager\RangeManager">
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument>%spomky_ip_filter.range.class%</argument>
        </service>
    </services>

</container>
