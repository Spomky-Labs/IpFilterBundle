{
    "name": "coosos/ip-filter-bundle",
    "type": "symfony-bundle",
    "description": "Symfony4 Ip Filter Bundle",
    "keywords": ["ip", "ipv4", "ipv6", "environment", "range", "filter", "Symfony4", "bundle"],
    "homepage": "https://github.com/Spomky/SpomkyLabsIpFilterBundle",
    "license": "MIT",
    "authors": [
        {
          "name": "Florent Morselli"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "^4.0",
        "doctrine/orm": "^2.6"
    },
    "require-dev": {
        "symfony/security-bundle": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/yaml": "^4.0",
        "symfony/browser-kit": "^4.0",
        "symfony/expression-language": "^4.0",
        "doctrine/doctrine-fixtures-bundle": "^2.4",
        "dama/doctrine-test-bundle": "^6.2",
        "symfony/console": "^4.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/dotenv": "^4.0"
    },
    "autoload": {
        "psr-4": { "SpomkyLabs\\IpFilterBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": {
            "SpomkyLabs\\AppIpFilterBundle\\": "Tests/src/",
            "SpomkyLabs\\TestIpFilterBundle\\": "Tests/tests/"
        }
    },
    "target-dir": "Coosos/IpFilterBundle"
}
